<% include ../partials/header %>
  
            <table class="table">
                <thead>
                    <tr>
                        <th>SID</th>
                        <th>Title</th>
                        <th>Firstname</th>
                        <th>Lastname</th>
                    </tr>
                </thead>
                <tbody>
                 <% students.forEach(function(student) { %>
                    <tr>
                        <td scope="row"><%= students.sid %></td>
                        <td><%= students.title %></td>
                        <td><%= students.firstname %></td>
                        <td><%= students.lastname %></td>
                    </tr>
                     <% }); %>
                </tbody>
            </table>


 <% include ../partials/footer %>